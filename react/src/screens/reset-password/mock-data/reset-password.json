{
  "client": {
    "id": "mockID",
    "name": "mockApplication"
  },
  "prompt": { "name": "reset-password" },
  "screen": {
    "name": "reset-password",
    "texts": {
      "pageTitle": "Reset your password | mockApplication",
      "title": "Change Your Password",
      "description": "Enter a new password below to change your password.",
      "buttonText": "Reset password",
      "passwordPlaceholder": "New password",
      "reEnterpasswordPlaceholder": "Re-enter new password",
      "passwordSecurityText": "Your password must contain:",
      "logoAltText": "test3",
      "showPasswordText": "Show password",
      "hidePasswordText": "Hide password",
      "accessibilityError": "Fail",
      "accessibilityValid": "Pass",
      "badgeUrl": "https://auth0.com/?utm_source=lock&utm_campaign=badge&utm_medium=widget",
      "badgeAltText": "Link to the Auth0 website",
      "error": "Error",
      "qrCode": "QR Code",
      "spinner_push_notification_label": "Waiting for push notification to be accepted"
    }
  },
  "tenant": { "mfa_options": { "pick_authenticator_enabled": true } },
  "transaction": {
    "connection": {
      "strategy": "auth0",
      "options": {
        "authentication_methods": {
          "password": {
            "enabled": true,
            "min_length": 8,
            "password_security_info": [
              {
                "code": "password-policy-length-at-least",
                "label": "At least 8 characters",
                "status": "error",
                "args": { "count": 8 }
              },
              {
                "code": "password-policy-contains-at-least",
                "label": "At least 3 of the following:",
                "status": "error",
                "items": [
                  {
                    "code": "password-policy-lower-case",
                    "label": "Lower case letters (a-z)",
                    "status": "error"
                  },
                  {
                    "code": "password-policy-upper-case",
                    "label": "Upper case letters (A-Z)",
                    "status": "error"
                  },
                  {
                    "code": "password-policy-numbers",
                    "label": "Numbers (0-9)",
                    "status": "error"
                  },
                  {
                    "code": "password-policy-special-characters",
                    "label": "Special characters (ex. !@#)",
                    "status": "error"
                  }
                ],
                "args": { "count": 3, "total": 4 }
              },
              {
                "code": "password-policy-identical-chars",
                "label": "No more than 2 identical characters in a row",
                "status": "error",
                "args": { "count": 2, "example": "aaa" }
              }
            ]
          }
        }
      }
    },
    "locale": "en",
    "state": "mockState"
  },
  "branding": {
    "themes": {
      "default": {
        "borders": {
          "buttons_style": "rounded",
          "inputs_style": "rounded",
          "button_border_weight": 1,
          "button_border_radius": 3,
          "input_border_weight": 1,
          "input_border_radius": 3,
          "widget_corner_radius": 5,
          "widget_border_weight": 0,
          "show_widget_shadow": true
        },
        "colors": {
          "primary_button": "#635dff",
          "primary_button_label": "#ffffff",
          "secondary_button_border": "#c9cace",
          "secondary_button_label": "#1e212a",
          "base_focus_color": "#635dff",
          "base_hover_color": "#000000",
          "links_focused_components": "#635dff",
          "header": "#1e212a",
          "body_text": "#1e212a",
          "widget_background": "#ffffff",
          "widget_border": "#c9cace",
          "input_labels_placeholders": "#65676e",
          "input_filled_text": "#000000",
          "input_border": "#c9cace",
          "input_background": "#ffffff",
          "icons": "#65676e",
          "error": "#d03c38",
          "success": "#13a688",
          "captcha_widget_theme": "auto"
        },
        "fonts": {
          "reference_text_size": 16,
          "links_style": "normal",
          "title": { "bold": false, "size": 150 },
          "subtitle": { "bold": false, "size": 87.5 },
          "body_text": { "bold": false, "size": 87.5 },
          "buttons_text": { "bold": false, "size": 100 },
          "input_labels": { "bold": false, "size": 100 },
          "links": { "bold": true, "size": 87.5 }
        },
        "page_background": {
          "page_layout": "center",
          "background_color": "#000000"
        },
        "widget": {
          "logo_position": "center",
          "logo_height": 52,
          "header_text_alignment": "center",
          "social_buttons_layout": "bottom"
        },
        "displayName": "Unnamed Theme"
      }
    },
    "settings": {
      "colors": { "page_background": "#000000", "primary": "#635dff" }
    }
  }
}
